import java.util.Scanner;
class hello{
    public static void main (String [] args){
     Scanner sc = new Scanner (System.in);
     int n = sc.nextInt();
     int arr [] = new int[n];
     for ( int i=0;i<=arr.length-1;i++){
        arr[i]=sc.nextInt();
     }
     int j =0;
     for(int i=1;i<=arr.length-1;i++){
        if(arr[j]!=arr[i]){
            j++;
            arr[j]=arr[i];
        }
     }
     for (int i=0;i<=j;i++){
        System.out.print(arr[i]);
     }
}
}
