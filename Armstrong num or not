import java.util.Scanner;
class hello{
    public static void main (String [] args){
     Scanner sc = new Scanner (System.in);
     int num = sc.nextInt();
     int originalNum=num;
     int sum=0;
     while(num>0){
        int r = num%10;
        sum=sum+(r*r*r);
        num = num/10;
     }
     if(sum==originalNum)
     System.out.println(originalNum + " it is an armstrong number");
     else
     System.out.println(originalNum + " it is not an armstrong number");
}
}
