import java.util.Scanner;
class hello{
    public static void main (String [] args){
     Scanner sc = new Scanner (System.in);
     int row = sc.nextInt();
     for( int i=0;i<=row;i++){
        for ( int j=0 ;j<=row-i;j++){
            System.out.print(" ");
        }
        int num=1; 
        for ( int k=0;k<=i;k++){
            System.out.print(num + " ");
           num = num * (i-k)/(k+1);
        }
       System.out.println(" ");
     }  
}
}
