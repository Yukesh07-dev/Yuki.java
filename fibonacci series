import java.util.Scanner;
class hello{
    public static void main (String [] args){
     Scanner sc = new Scanner (System.in);
     int n = sc.nextInt();
     int num1=0;
     int num2=1;
     for (int i=0;i<=n;i++){
        int num3=num1+num2;
        System.out.print(num3 + " ");
        num1=num2;
        num2=num3;
     }
}
}
